#define APSTUDIO_READONLY_SYMBOLS
#define APSTUDIO_HIDDEN_SYMBOLS
#include <Windows.h>
#include <winuser.h>
#undef APSTUDIO_HIDDEN_SYMBOLS
#undef APSTUDIO_READONLY_SYMBOLS

CREATEPROCESS_MANIFEST_RESOURCE_ID RT_MANIFEST "tek-s3.manifest"
VS_VERSION_INFO VERSIONINFO
FILEVERSION     @file_version@
PRODUCTVERSION  @file_version@
FILEFLAGSMASK   0
FILEFLAGS       0
FILEOS          VOS_NT
FILETYPE        VFT_APP
FILESUBTYPE     0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "00000000"
    BEGIN
      VALUE "CompanyName", "TEKnology Hub"
      VALUE "FileDescription", "TEK Steam Sharing Server"
      VALUE "FileVersion", "@pretty_version@"
      VALUE "InternalName", "tek-s3"
      VALUE "LegalCopyright", "Copyright (c) 2025 Nuclearist"
      VALUE "OriginalFilename", "tek-s3.exe"
      VALUE "ProductName", "TEK Steam Sharing Server"
      VALUE "ProductVersion", "@pretty_version@"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0, 0
  END
END
